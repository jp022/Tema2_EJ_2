package ejercicio2;

public class Card {//Esta clase crea una carta con un palo y un valor

	public String suit;//Esta variable define el palo
	public String value;//Esta variable define el valor
	
	public Card (String suit, String value) {//Este es el constructor del objeto carta
		this.suit = suit;
		this.value = value;
	}
	
	public String toString () {//Este metodo devuelve la carta como un String.
		return (this.suit+"-"+this.value);
	}
}
